----------------------+            elif method.upper() == "DELETE":
----------------------+                response = requests.delete(url, headers=headers, timeout=30)
----------------------+            else:
----------------------+                raise ValueError(f"Unsupported method: {method}")
----------------------+                
----------------------+            return response
----------------------+            
----------------------+        except requests.exceptions.RequestException as e:
